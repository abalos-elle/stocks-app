<h2>Owned Stocks </h2>

<table class="table table-primary table-striped">
  <thead>
    <tr class="table-primary">
      <th>Name</th>
      <th>Symbol</th>
      <th>Price</th>
      <th>Percent Change</th>
    </tr>
  </thead>
  <tbody>
    <% @owned_stocks.each do |cmp| %>
      <tr>
        <td class="table-primary">
          <%= cmp.user_id %>
        </td>
        <td class="table-primary">
          <%= cmp.company_id     %>
        </td>
        <td class="table-primary">
          <%= cmp.price %>
        </td>
        <td class="table-primary">
          <%= cmp.buy_date %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>